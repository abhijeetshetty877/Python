fruit = []
print(fruit)

    
Ran = int(input("Enter the range : "))

for i in range(Ran):
    value=input(f"Enter a fruit name {i+1}: ")
    fruit.append(value)

print(fruit)

add1 = input("Add fruit name  : ")
fruit.insert(1,add1)
print(fruit)

add2 = input("Add fruit name at last: ")
fruit.append(add2)
print(fruit)

fruit.pop()
print(fruit)

remo = input("Add name to delete: ")
fruit.remove(remo)
print(fruit)

remo = int(input("Add value to delete: "))
fruit.pop(remo)
print(fruit)

add = input("Add fruit name : ")
fruit.append(add)
print(fruit)
